<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment>Database Configuration</comment>
	<entry key="database ip">192.168.0.100</entry>
	<entry key="database port">3306</entry>
	<entry key="jdbc driver name">com.mysql.jdbc.Driver</entry>
	<entry key="pool driver name">org.apache.commons.dbcp.PoolingDriver</entry>
	<entry key="pool name">connectPool</entry>
	<entry key="database schema">huawei</entry>
	<entry key="user">huawei</entry>
	<entry key="password">huawei</entry>
	<entry key="characterEncoding">utf-8</entry>
	<entry key="useUnicode">true</entry>
	<entry key="max active num">10</entry>
	<entry key="max idle num">5</entry>
	<entry key="max wait num">100</entry>
</properties>